{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "types": ["svelte"],
    "verbatimModuleSyntax": true,
    "isolatedModules": true,
    "noEmit": true,
    "lib": ["es2022", "dom", "dom.iterable"],
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true
  }
}
